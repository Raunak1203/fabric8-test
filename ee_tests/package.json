      "email": "sudsen@redhat.com"
    }
  ],
  "homepage": "https://github.com/fabric8io/fabric8-test",
  "license": "Apache-2.0",
  "scripts": {
    "postinstall": "webdriver-manager update && tsc &&  webdriver-manager update --versions.chrome 2.29",
    "image:build": "docker build -t fabric-ui-test:latest -f Dockerfile.builder .",
    "protractor": "protractor",
    "tsc": "tsc",
    "tscw": "tsc -w",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "webdriver:start": "webdriver-manager start --versions.chrome 2.29",
    "webdriver:update": "webdriver-manager update"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.47",
    "@types/lodash": "4.14.61",
    "@types/node": "7.0.12",
    "jasmine-spec-reporter": "4.2.1",
    "jasmine-protractor-matchers": "^1.0.1",
    "jasmine2-reporter": "^0.3.0",
    "jasmine-reporters": "^2.2.1",
    "protractor": "^5.1.1",
    "protractor-element-extend": "^2.0.1",
    "protractor-jasmine2-screenshot-reporter": "0.3.5",
    "rimraf": "^2.6.2",
    "ts-helpers": "1.1.2",
    "ts-node": "3.0.2",
    "tslint": "5.0.0",
    "tslint-loader": "^3.5.0",
    "typescript": "2.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabric8io/fabric8-test.git"
  },
  "bugs": {
    "url": "https://github.com/fabric8io/fabric8-test/issues"
  },
  "engines": {
    "node": ">= 6.11.4",
    "npm": ">= 3"
  },
  "release": {
    "branch": "master",
    "verifyConditions": {
      "path": "./node_modules/@krux/condition-jenkins"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
