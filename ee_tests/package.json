{
  "name": "fabric8-ui-tests",
  "version": "0.0.0-development",
  "description": "End to End tests for fabric8",
  "keywords": [
    "e2e",
    "fabric8",
    "tests"
  ],
  "contributors": [
    {
      "name": "Joshua Wilson",
      "email": "josh@redhat.com"
    },
